{
  "name": "@livecalc/benchmarks",
  "version": "1.0.0",
  "description": "Performance benchmarking suite for LiveCalc engine",
  "type": "module",
  "private": true,
  "scripts": {
    "benchmark": "tsx run-benchmarks.ts",
    "benchmark:single": "tsx run-benchmarks.ts --no-multi",
    "benchmark:multi": "tsx run-benchmarks.ts --no-single --no-native",
    "benchmark:ci": "tsx run-benchmarks.ts --ci",
    "benchmark:quick": "tsx run-benchmarks.ts --no-native",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@livecalc/engine": "file:../js"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
