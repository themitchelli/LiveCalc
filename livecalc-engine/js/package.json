{
  "name": "@livecalc/engine",
  "version": "1.0.0",
  "description": "JavaScript wrapper for LiveCalc WASM projection engine with Web Worker support",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./worker": {
      "types": "./dist/worker.d.ts",
      "import": "./dist/worker.mjs",
      "require": "./dist/worker.js"
    },
    "./node-worker": {
      "types": "./dist/node-worker.d.ts",
      "import": "./dist/node-worker.mjs",
      "require": "./dist/node-worker.js"
    }
  },
  "files": [
    "dist",
    "wasm"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/worker.ts src/node-worker.ts --format cjs,esm --dts --clean",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "livecalc",
    "wasm",
    "actuarial",
    "projection",
    "valuation",
    "web-worker",
    "parallel",
    "shared-array-buffer",
    "zero-copy"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
